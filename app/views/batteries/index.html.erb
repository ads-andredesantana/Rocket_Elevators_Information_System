<p id="notice"><%= notice %></p>

<h1>Batteries</h1>

<table>
  <thead>
    <tr>
      <th>Building</th>
      <th>Propertytype</th>
      <th>Status</th>
      <th>Employee</th>
      <th>Dateofcommissioning</th>
      <th>Dateoflastinspect</th>
      <th>Certificateofoperation</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @batteries.each do |battery| %>
      <tr>
        <td><%= battery.building %></td>
        <td><%= battery.PropertyType %></td>
        <td><%= battery.Status %></td>
        <td><%= battery.employee %></td>
        <td><%= battery.DateOfCommissioning %></td>
        <td><%= battery.DateOfLastInspect %></td>
        <td><%= battery.CertificateOfOperation %></td>
        <td><%= link_to 'Show', battery %></td>
        <td><%= link_to 'Edit', edit_battery_path(battery) %></td>
        <td><%= link_to 'Destroy', battery, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Battery', new_battery_path %>
